    "version": "0.2.0",
    "configurations": [

        {
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}\\target\\thumbv7em-none-eabihf\\gdb\\Rust_Embedded_Template_VSC.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "device": "STM32G431R8",
            "svdFile": "STM32G431.svd",
            "svdPath": "D:\\jbolivarg\\EmbeddedProjects\\Rust\\rust-embedded-template-vsc",
            "preLaunchTask": "Flash with flasher.py",
            "serverArgs": [
                "-if", "SWD",
                "-speed", "4000",
                "-noir",
                "-endian", "little",
                "-timeout", "0",
                "-vd"
            ],
            "interface": "swd",
            "swoConfig": {
                "enabled": false
            },
            "showDevDebugOutput": "raw"
        },
        {
            "name": "Flash & Debug with flasher.py",
